teken = input("Geef teken in: ")
rij_i = int(input("Geef aantal rijen in: "))
kolom = int(input("Geef aantal kolommen in: "))

for i in range(rij_i):  # Loop door het aantal rijen
    for j in range(kolom):  # Loop door het aantal kolommen
        print(teken, end="\t")  # Print het teken met een tab als scheiding
    print()  # Ga naar de volgende rij


teken = input("Geef teken in: ")
rij_i = int(input("Geef aantal rijen in: "))
kolom = int(input("Geef aantal kolommen in: "))

for i in range(rij_i):
    for j in range(kolom-i):
        print(teken, end="\t")
    print()

matrix=[[6,8,5,2,4],[5,1,5,2,9],[7,2,7,2,6] ]

for rij in matrix:
    for item in rij:
        print(item,end="\t")
    print() #enter om naar volgende rij te gaan

#  Genereer een tafelkaart in een 2d array


tafel = []

for i in range(1, 11):
    rij = []
    for j in range(1, 11):
        rij.append(i * j)
    tafel.append(rij)

for rij in tafel:
    for item in rij:
        print(item,end="\t")
    print()
